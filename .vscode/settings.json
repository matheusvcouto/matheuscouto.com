{
  "workbench.colorTheme": "Min Dark",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true
  },
  "tailwindCSS.experimental.classRegex": [
    ["cn\\(([^)]*)\\)", "'([^']*)'"],
    // ["tw\\(([^)]*)\\)", "'([^']*)'"]
    ["tw`([^`]*)", ""]
  ],
}
